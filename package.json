{
  "name": "msarchive-v2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "typecheck": "tsc --noEmit",
    "build": "pnpm run typecheck && next build",
    "export": "next build",
    "start": "npx serve out",
    "lint": "next lint",
    "lint:schema": "rg -n \"\\.from\\(['\"]incidents['\"]\\)\" --glob '!**/node_modules/**' --glob '!**/.next/**' | (! rg -v \"schema\\('data'\\)\\.from\\(['\"]incidents['\"]\\)\" ) && echo \"ERROR: found .from('incidents') without schema('data')\" && exit 1 || echo OK",
    "prebuild": "pnpm run lint:schema"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@supabase/ssr": "^0.7.0",
    "next": "^15.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.1.3"
  }
}
